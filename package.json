{
  "name": "cobalt",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "dotenv -e .env.development.local -- remix dev --manual",
    "migrate": "dotenv -e .env.development.local -- npx prisma migrate dev",
