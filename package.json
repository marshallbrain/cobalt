    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "eventemitter3": "^5.0.1",
    "isbot": "^3.6.8",
    "kysely": "^0.26.3",
    "mime-types": "^2.1.35",
    "pg": "^8.11.3",
    "prisma": "^5.5.2",
    "prisma-kysely": "^1.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.1.0",
    "uuid": "^9.0.1",
    "sharp": "^0.32.6"
